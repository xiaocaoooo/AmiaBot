{
  "compilerOptions": {
    "target": "ES2017",
    "module": "CommonJS",
    "lib": [
      "ES2017",
      "DOM",
      "DOM.Iterable"
    ],
    "outDir": "webui/static/js",
    "rootDir": "webui/static/ts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "webui/static/ts/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
